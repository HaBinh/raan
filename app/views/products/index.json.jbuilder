json.products @products.each do |product| 
  json.id product.product["id"]
  json.name product.product["name"]
  json.code product.product["code"]
  json.unit product.product["unit"]
  json.default_imported_price product.product["default_imported_price"]
  json.default_sale_price product.product["default_sale_price"]
  json.rates product.rates.sort
end